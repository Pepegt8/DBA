	[IdConfiguracionPallet] [int] NOT NULL,
	[LongCodBodegaOrigen] [int] NULL,
	[LongCodProducto] [int] NULL,
	[LongLP] [int] NULL,
	[CodigoNumerico] [bit] NULL,
